set number
set autoindent
filetype indent on
set shiftround
set shiftwidth=4
set tabstop=4
set hlsearch
set ignorecase
set smartcase
set incsearch
set showmode
set showmatch
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
syntax enable
